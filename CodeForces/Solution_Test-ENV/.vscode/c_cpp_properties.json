{
  "version": 4,
  "configurations": [
    {
      "name": "macOS",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "/usr/local/include",
        "/opt/homebrew/include",
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include/c++/v1"
      ],
      "defines": ["_DEBUG", "__APPLE__", "__MACH__"],
      "macFrameworkPath": [
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/usr/bin/clang++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "macos-clang-arm64",
      "compileCommands": "${workspaceFolder}/compile_commands.json",
      "configurationProvider": "ms-vscode.cmake-tools"
    },
    {
      "name": "macOS (Homebrew)",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "/opt/homebrew/include",
        "/usr/local/include",
        "/usr/include"
      ],
      "defines": ["_DEBUG", "__APPLE__", "__MACH__", "HOMEBREW"],
      "macFrameworkPath": [
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/opt/homebrew/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "macos-gcc-arm64",
      "compileCommands": "${workspaceFolder}/compile_commands.json",
      "configurationProvider": "ms-vscode.cmake-tools"
    },
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "/usr/include",
        "/usr/local/include",
        "/usr/include/c++/*",
        "/usr/include/x86_64-linux-gnu/c++/*"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE", "__linux__"],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "${default}",
      "compileCommands": ["${workspaceFolder}/compile_commands.json"],
      "configurationProvider": "ms-vscode.cmake-tools",
      "browse": {
        "path": ["${workspaceFolder}", "/usr/include", "/usr/local/include"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    },
    {
      "name": "Windows",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/*/include",
        "C:/Program Files (x86)/Windows Kits/10/Include/*/ucrt",
        "C:/Program Files (x86)/Windows Kits/10/Include/*/um",
        "C:/MSYS2/mingw64/include/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "WIN32",
        "_WIN32",
        "_WIN64",
        "NOMINMAX"
      ],
      "windowsSdkVersion": "10.0.22621.0",
      "compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/14.37.32822/bin/Hostx64/x64/cl.exe",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "windows-msvc-x64",
      "compileCommands": ["${workspaceFolder}/build/compile_commands.json"],
      "configurationProvider": "ms-vscode.cmake-tools"
    },
    {
      "name": "Windows (MinGW)",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "C:/MSYS2/mingw64/include",
        "C:/MSYS2/mingw64/include/c++/*",
        "C:/MSYS2/mingw64/lib/gcc/x86_64-w64-mingw32/*/include/c++"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "__MINGW32__",
        "__MINGW64__"
      ],
      "compilerPath": "C:/MSYS2/mingw64/bin/g++.exe",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "windows-gcc-x64",
      "compileCommands": ["${workspaceFolder}/build/compile_commands.json"],
      "configurationProvider": "ms-vscode.cmake-tools"
    }
  ]
}
